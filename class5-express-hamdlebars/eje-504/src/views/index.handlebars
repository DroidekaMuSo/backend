<div>
    <h1>Hi, welcome {{user.name}}</h1>

    {{#if user.role}}

        {{#each food}}
            <div>
                <p>{{this.name}}</p>
                <p> {{this.price}}</p>
            </div>
        {{/each}}

    {{else}}
        <p>User not allowed to see the list food</p>

    {{/if}}
</div>